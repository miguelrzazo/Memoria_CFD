// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType RAS;


RAS
{
    // High-Reynolds: Standard k-epsilon with wall functions
    model           kEpsilon;
    turbulence      on;
    viscosityModel  Newtonian;
    printCoeffs     on;
}



laminar
{
    model               Maxwell;

    MaxwellCoeffs
    {
        nuM             1;
        lambda          5;
    }

}

