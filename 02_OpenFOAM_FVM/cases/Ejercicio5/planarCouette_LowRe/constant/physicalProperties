// Re = 5e5 + 7*5000 = 535000 (ultima cifra DNI = 7)
// U = 10 m/s, H = 0.1 m
// nu = U*H/Re = 10*0.1/535000 = 1.869e-6 m2/s

viscosityModel  constant;

nu              1.869e-06;

