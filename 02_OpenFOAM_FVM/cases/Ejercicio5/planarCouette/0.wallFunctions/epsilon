FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 14.855;

boundaryField
{

    movingWall
    {
        type            epsilonWallFunction;
        value           uniform 14.855;
    }
    bottomWall
    {
        type            epsilonWallFunction;
        value           uniform 14.855;
    }
    #includeEtc "caseDicts/setConstraintTypes"

}


