FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

// Velocidad u = 2.5 m/s (Caso 2 del enunciado)
internalField   uniform (2.5 0 0);

boundaryField
{
    in
    {
      type            fixedValue;
      value $internalField;
    }
    out
    {
        type            fixedValue;
	value $internalField;
    }

    emptywalls
    {
        type            empty;
    }
}


// ************************************************************************* //
