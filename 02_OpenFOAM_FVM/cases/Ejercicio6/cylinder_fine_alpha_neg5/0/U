FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

Uinlet          1.0;

// Angulo de ataque alpha = -5 grados
// cos(-5) = 0.99619, sin(-5) = -0.08716
alpha           -0.08727;  // -5 deg in radians
Uinlet_x          0.99619;
Uinlet_y          -0.08716;



dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform ($Uinlet_x $Uinlet_y 0);
    }

    outlet
    {
        type            zeroGradient;
    }

    wall
    {
        type            noSlip;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}

// ************************************************************************* //
